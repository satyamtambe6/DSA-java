import java.util.Scanner;

class MissingNumber {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        // input n
        int n = sc.nextInt();

        int[] arr = new int[n - 1];
        int sum = 0;

        // input array elements
        for (int i = 0; i < n - 1; i++) {
            arr[i] = sc.nextInt();
            sum += arr[i];
        }

        int expectedSum = n * (n + 1) / 2;
        int missingNumber = expectedSum - sum;

        System.out.println(missingNumber);
    }
}
