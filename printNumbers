public class Main {

    // Recursive function
    static void printNumbers(int n) {

        // Base case
        if (n == 0) {
            return;
        }

        // Recursive call
        printNumbers(n - 1);

        // Print after recursive call
        System.out.print(n + " ");
    }

    public static void main(String[] args) {
        int n = 5;
        printNumbers(n);
    }
}
